<?xml version="1.0" encoding="UTF-8"?>
<customMapSource>
    <name>CalTopo</name>
    <!-- http://caltopo.com/map.html -->
    <minZoom>4</minZoom>
    <maxZoom>16</maxZoom>
    <tileType>png</tileType>
    <tileUpdate>ifNoneMatch</tileUpdate> <!-- tested -->
    <url>http://s3-us-west-1.amazonaws.com/caltopo/topo/{$z}/{$x}/{$y}.png</url>
    <backgroundColor>#FFFFFFFF</backgroundColor>
</customMapSource>
